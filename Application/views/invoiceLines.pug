if invoiceLine.length !== 0
  each item, idx in invoiceLine
    if '明細-項目ID' in item
      .box
        .columns.m-0
          - for (var [key,value] of Object.entries(item))
            if key === '明細-項目ID' && value.length !== 0
              .column-header.is-2-header
                p.text-left-lines.font-bold 項目ID
                p.text-left-lines.mb-1= value
            if key === '明細-内容' && value.length !== 0
              .column-header.is-5-header
                p.font-bold  内容
                each subItem in value
                  if subItem.item === '内容'
                    p.mb-1= subItem.value
                  else
                    p.mb-1= `${subItem.item}： ${subItem.value}`
            if key === '明細-数量' && value.length !== 0
              .column-header.mr-1
                p.text-right-lines.font-bold  数量
                each subValue in value
                  if subValue !== 1
                    p.text-right-lines.mb-1= subValue
                  else
                    p.text-right-lines.mb-1= '　'

            if key === '明細-単位' && value.length !== 0
              .column-header.ml-1
                p.text-left-lines.font-bold 単位
                each subValue in value
                  if subValue !== 1
                    p.text-left-lines.mb-1= subValue
                  else
                    p.text-left-lines.mb-1= '　'
            if key === '明細-単価' && value.length !== 0
              .column-header
                p.text-right-lines.font-bold  単価
                each subValue in value
                  p.text-right-lines.mb-1= subValue
            if key === '明細-税（消費税／軽減税率／不課税／免税／非課税）' && value.length !== 0
              .column-header
                p.text-right-lines.font-bold  税
                p.text-right-lines.mb-1= value     
            if key === '明細-小計 (税抜)' && value.length !== 0
              .column-header
                p.text-right-lines.font-bold  小計 (税抜) 
                p.text-right-lines.mb-1= value
        .columns
          if item.taxTotal.taxSubtotal.category === 'G' || item.taxTotal.taxSubtotal.category === 'Z'
            .column-header
              p.text-left-lines #{item.taxTotal.taxSubtotal.name}
        .columns.m-0
          .column.is-2-header
            .column-header.text-inbox.text-inbox-subtitle.text-inbox-common
              | 仕訳情報
              a.btn-plus-accountCode(data-target= `#lineNo${idx + 1}`)
                i.fas.fa-plus-circle
        .columns.m-0
          .column.p-0.lineAccountCode(id= `lineNo${idx + 1}`)
            .columns.m-0.p-1(id= `lineNo${idx + 1}_lineAccountCode1`)
              .column.is-one-third.p-0.border-div-rad-4
                .field.is-horizontal.m-1.has-addons-centered
                  .field-label.is-normal.m-0.text-center
                    label 勘定科目コード
                  .field-label.is-normal.m-0.text-center
                    label 補助科目コード
                  .field-label.is-normal.m-0.is-1.fixed-void-66
                    label 　
                .field.is-horizontal.p-1
                  .field-body.m-1
                    .field
                      p.control
                        input.input(type='text' name=`lineNo${idx + 1}_accountCode_1` readonly)
                  .field-body.m-1
                    .field
                      p.control
                        input.input(type='text' name=`lineNo${idx + 1}_subAccountCode_1` readonly)
                  .field-body.m-1.is-1.none-flex-grow
                    .field
                      p.control
                        button.button.is-link.show-modal(data-target='accountCode-modal') 検索
              .column.is-0.p-1
              .column.is-one-quarter.p-0.border-div-rad-4
                .field.is-horizontal.m-1.has-addons-centered
                  .field-label.is-normal.m-0.text-center
                    label 分割金額
                  .field-label.is-normal.m-0.is-1.fixed-void-66
                    label 　
                .field-body.p-1
                  .field.is-horizontal
                    .field-body.m-1
                      .field
                        p.control
                          input.input(type='text' id=`lineNo${idx + 1}_lineAccountCode1_input_amount` name=`lineNo${idx + 1}_lineAccountCode1_input_amount` readonly value= value)
                    .field-body.m-1.is-1.none-flex-grow
                      .field
                        p.control

              .column
                a.red-color(id=`btn-minus-lineNo${idx + 1}-accountCode`)
                  i.fas.fa-minus-circle
    else
      .box
        - for (var [key,value] of Object.entries(item))
          - var idx = 0
          each subitem in value
            div.paymentmean
              .columns.m-0
                - for (var [subKey,subValue] of Object.entries(subitem))
                    if (subKey === '割引-項目ID'||subKey === '追加料金-項目ID') && subValue.length !== 0
                      .column-header.is-2-header
                        p.text-left-lines= subValue
                    if (subKey === '割引-内容'||subKey === '追加料金-内容') && subValue.length !== 0
                      .column-header.is-5-header
                        p= subValue
                    if (subKey === '割引-数量'||subKey === '追加料金-数量') && subValue.length !== 0
                      .column-header.mr-1
                        p.text-right-lines= subValue
                    if (subKey === '割引-単位'||subKey === '追加料金-単位') && subValue.length !== 0
                      .column-header.ml-1
                        p.text-left-lines= subValue
                      .column-header
                        p 　
                    if (subKey === '割引-税（消費税／軽減税率／不課税／免税／非課税）'||subKey === '追加料金-税（消費税／軽減税率／不課税／免税／非課税）') && subValue.length !== 0
                      .column-header
                        p.text-right-lines= subValue
                    if (subKey === '割引-小計（税抜）'||subKey === '追加料金-小計（税抜）') && subValue.length !== 0
                      .column-header
                        p.text-right-lines= subValue
              .columns.m-0
                if allowanceCharge[idx]
                  if allowanceCharge[idx].taxCategoryId === 'G' || allowanceCharge[idx].taxCategoryId === 'Z'
                    .column-header
                      | #{allowanceCharge[idx].taxCategory}
              - idx++